
# OpenAI
OPENAI_API_KEY=your_openai_api_key

# Airtable
# Get your API key from https://airtable.com/account
AIRTABLE_API_KEY=your_airtable_api_key

# Base ID format: appXXXXXXXXXXXXXX
# Find in your Airtable workspace URL: airtable.com/appXXXXXXXX/...
# Make sure you're using the correct Base ID, not the workspace ID
AIRTABLE_BASE_ID=your_airtable_base_id

# The exact name of your table (case-sensitive)
# Make sure your table has these fields:
# - Name (Single line text)
# - Image (Attachment type)
# - Type (Single select)
# - Upload Date (Date)
# - Recipe Name (Single line text)
# - Recipe Category (Single select)
# - Ingredients (Long text)
# - Preparation Steps (Long text)
AIRTABLE_TABLE_NAME=your_airtable_table_name
